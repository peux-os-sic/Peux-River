[
  {
    "label": "Lock",
    "exec": "~/.config/system_scripts/wayland_session_lock &",
    "icon": "lock"
  },
  {
    "label": "Logout",
    "exec": "killall wayfire || killall river || killall sway",
    "icon": "system-log-out"
  },
  {
    "label": "Hibernate",
    "exec": "systemctl hibernate",
    "icon": "system-hibernate"
  },
  {
    "label": "Suspend",
    "exec": "systemctl suspend",
    "icon": "system-suspend"
  },
  {
    "label": "Reboot",
    "exec": "systemctl reboot",
    "icon": "system-reboot"
  },
  {
    "label": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "system-shutdown"
  }
]
